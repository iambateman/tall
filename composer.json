{
    "name": "laravel-frontend-presets/tall",
    "description": "TALL preset for Laravel.",
    "keywords": [
        "alpine",
        "alpinejs",
        "tailwind",
        "tailwindcss",
        "tailwindui",
        "tall",
        "laravel",
        "livewire",
        "preset"
    ],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/laravel-frontend-presets/tall/issues",
        "source": "https://github.com/laravel-frontend-presets/tall"
    },
    "authors": [
        {
            "name": "Stephen Bateman",
            "email": "stephen@bate-man.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/ui": "^4.2",
        "illuminate/support": "^10.0",
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.10"
    },
    "autoload": {
        "psr-4": {
            "LaravelFrontendPresets\\Tall\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaravelFrontendPresets\\Tall\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelFrontendPresets\\Tall\\TallServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
